#include<stdio.h>
int main(){
    
    int n,a[10],sum=0,i;
    
    scanf("%d",&n);
    
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    
    for(i=0;i<n;i++){
       
            sum += a[i];
        
    }
    
    int avg = sum / n;
    
    printf("%d",avg);
    
    return 0;
    
}
