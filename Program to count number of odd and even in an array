#include<stdio.h>
int main(){
    
    int n,arr[5],even=0,odd=0,i;
    scanf("%d",&n);
    
    
    for(i = 0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    
    
    for(i=0;i<n;i++){
        if(arr[i] % 2 == 0){
            even++;
        }
        else{
            odd++;
        }
    }
    printf("%d even \n",even);
    
    printf("%d odd \n",odd);
    
    return 0;
    
}
