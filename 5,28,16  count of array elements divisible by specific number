
#include<stdio.h>
int main(){
    
    int n;
    scanf("%d",&n);
    int num;
    printf("Enter the number to check the count: "); scanf("%d", &num);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int count = 0;
    for(int i=0;i<n;i++){
        if(arr[i] % num == 0) count++;
    }
    printf("%d", count);
    
}
