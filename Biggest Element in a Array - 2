#include<stdio.h>
int main(){
    
    int i,n,arr[5],big;
    
    scanf("%d",&n);
    
    
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    
    big = arr[0];
    
    for(i=1;i<n;i++){
        if(arr[i] > big){
            big = arr[i];
        }
    }
    printf("%d is big",big);
    
    
}
